<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageId>RxTelegram.Bot</PackageId>
        <Version>0.0.1</Version>
        <Authors>RxTelegram.Bot</Authors>
        <TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
        <Description>Reactive Telegram Bot API for .NET</Description>
        <PackageLicenseUrl>https://github.com/RxTelegram/RxTelegram.Bot/blob/master/LICENSE.md</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/RxTelegram/RxTelegram.Bot</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>Telegram;Bot;Api;Rx;Reactive;Observable</PackageTags>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    </ItemGroup>

</Project>
